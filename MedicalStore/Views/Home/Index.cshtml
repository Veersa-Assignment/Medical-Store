@model StoreKeeper;
<nav class="navbar navbar-expand-lg navbar-dark bg-primary">
    <div class="container-fluid">
        <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">Medical Store</a>
        <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarColor01" aria-controls="navbarColor01" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarColor01">
            <ul class="navbar-nav me-auto">
                <li class="nav-item">
                    <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                </li>
               

            </ul>
            <form class="d-flex">

                <button class="btn btn-secondary my-2 my-sm-0" type="submit">  <a style="text-decoration:none" class="nav-link text-dark" asp-area="" asp-controller="StoreManager" asp-action="Index">Login</a></button>
                @*<button class="btn btn-secondary my-2 my-sm-0" type="submit"><a asp-area="" style="text-decoration:none" asp-controller="StoreKeeper" asp-action="Index">Login as Store Keeper</a></button>*@
