@model IEnumerable<Pending>
<h2>Pendings</h2>
<table>
    <thead>
        <tr>
            <th>
                Medicine
            </th>
            <th>
                Stock in
            </th>
            <th>
                Stock out
            </th>
            <th>
                Expired
            </th>
            <th>
                Final
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var obj in Model)
        {
            <tr>
                <td>
                    @obj.MedicineName
                </td>
                <td>
                    @obj.StockIn
                </td>
                <td>
                    @obj.StockOut
                </td>
                <td>
                    @obj.Expired
                </td>
                <td>
                    @obj.Final
                    
                </td>
                <td>
                    <a asp-controller="StoreManager" asp-action="Approved" asp-route-id="@obj.id"  >Approve</a>
                </td>
            </tr>
        }
    </tbody>
</table>
